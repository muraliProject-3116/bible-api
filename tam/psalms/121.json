     "verses":  [
        {
            "reference":  "சங்கீதம் 121:1",
             "verseNumber":  1,
             "scripture":  "எனக்கு ஒத்தாசை வரும் பர்வதங்களுக்கு நேராக என் கண்களை ஏறெடுக்கிறேன்."
        },
        {
            "reference":  "சங்கீதம் 121:2",
             "verseNumber":  2,
             "scripture":  "வானத்தையும் பூமியையும் உண்டாக்கின கர்த்தரிடத்திலிருந்து எனக்கு ஒத்தாசை வரும்."
        },
        {
            "reference":  "சங்கீதம் 121:3",
             "verseNumber":  3,
             "scripture":  "உன் காலைத் தள்ளாடவொட்டார்; உன்னைக் காக்கிறவர் உறங்கார்."
        },
        {
            "reference":  "சங்கீதம் 121:4",
             "verseNumber":  4,
             "scripture":  "இதோ, இஸ்ரவேலைக் காக்கிறவர் உறங்குகிறதுமில்லை தூங்குகிறதுமில்லை."
        },
        {
            "reference":  "சங்கீதம் 121:5",
             "verseNumber":  5,
             "scripture":  "கர்த்தர் உன்னைக் காக்கிறவர்; கர்த்தர் உன் வலது பக்கத்திலே உனக்கு நிழலாயிருக்கிறார்."
        },
        {
            "reference":  "சங்கீதம் 121:6",
             "verseNumber":  6,
             "scripture":  "பகலிலே வெயிலாகிலும், இரவிலே நிலவாகிலும் உன்னைச் சேதப்படுத்துவதில்லை."
        },
        {
            "reference":  "சங்கீதம் 121:7",
             "verseNumber":  7,
             "scripture":  "கர்த்தர் உன்னை எல்லாத் தீங்குக்கும் விலக்கிக் காப்பார்; அவர் உன் ஆத்துமாவைக் காப்பார்."
        },
        {
            "reference":  "சங்கீதம் 121:8",
             "verseNumber":  8,
             "scripture":  "கர்த்தர் உன் போக்கையும் உன் வரத்தையும் இதுமுதற்கொண்டு என்றைக்குங் காப்பார்."
        }
    ]
